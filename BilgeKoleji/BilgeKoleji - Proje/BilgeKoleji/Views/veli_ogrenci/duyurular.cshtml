@model IEnumerable<BilgeKoleji.Models.duyuru>

@{
    ViewBag.Title = "Index";
}

<div class="breadcrumbs ace-save-state" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="~/anasayfa/index">Anasayfa</a>
        </li>
        <li class="active">Duyurular</li>
    </ul>
</div>

<div class="page-content">

    <div class="row">
        <div class="col-xs-12">
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.mesaj)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tarihi)
                    </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.mesaj)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tarihi)
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>